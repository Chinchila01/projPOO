        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_iQZjtRnTEeaqg9F32W2naQ" name="Advisor" specification="_iI-GABnTEeaqg9F32W2naQ">
          <ownedParameter xmi:id="_iQZjthnTEeaqg9F32W2naQ" name="minBet">
